@model restaurante.Models.Reporte

@{
    ViewData["tilte"] = "ProductoMasVendido";
    Layout = "";
}


    <div class="row">
        <h2>Lista de Productos Vendidos por fecha:</h2>
        <div class="form-group col">
            <label asp-for="FechaInicio"></label>
            <input asp-for="FechaInicio" type="date" class="form-control" />
            <span asp-validation-for="FechaInicio" class="text-danger"></span>
        </div>
        <div class="form-group col">
            <label asp-for="FechaFin"></label>
            <input asp-for="FechaFin" type="date" class="form-control" />
            <span asp-validation-for="FechaInicio" class="text-danger"></span>
        </div>
        <div class="form-group col">
            <label class="col-12" style="color: rgba(0,0,0,0)">.</label>
            <button type="submit" class="btn btn-warning col">Imprimir</button>
        </div>
    </div>

